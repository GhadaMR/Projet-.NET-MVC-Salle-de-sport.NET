@model ProjetC_MVCSalleSport.Models.Coach

@using (Html.BeginForm("LoginCoach", "Coach", FormMethod.Post))
{
    <h2>Login As Coach</h2>
    <div>
        <label for="mail">Email:</label>
        @Html.TextBoxFor(model => model.mail, new { id = "mail" })
    </div>

    <div>
        <label for="motDePasse">Mot de passe:</label>
        @Html.PasswordFor(model => model.motDePasse, new { id = "motDePasse" })
    </div>

    <button type="submit">Se connecter</button>
}
